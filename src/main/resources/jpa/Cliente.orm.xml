<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm 
        http://java.sun.com/xml/ns/persistence/orm_1_0.xsd"
    version="1.0">

    <mapped-superclass class="residenciaAnimales.Cliente" access="FIELD">
        <attributes>
            <basic name="dni" optional="false" />
            <basic name="nombre" optional="false" />
            <basic name="apellido1" optional="false" />
            <basic name="apellido2" optional="true" />
            <basic name="telefono" optional="false" />
            <basic name="email" optional="true" />
            <one-to-many name="mascotas" target-entity="es.puentes.repositorios.MascotaConId" mapped-by="cliente"/>
        </attributes>
    </mapped-superclass>
</entity-mappings>